{
  "manifest_version": 3,
  "name": "Toolbar Dial",
  "version": "2.4",
  "permissions": ["bookmarks", "storage"],
  "description": "Toolbar Dial is an extension for modern browsers that replaces the new tab page with a colorful grid of your bookmarks and folders.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },
  "icons": {
    "16": "16.png",
    "32": "32.png",
    "48": "48.png",
    "64": "64.png",
    "96": "96.png",
    "128": "128.png",
    "256": "256.png",
    "512": "512.png",
    "1024": "1024.png"
  },
  "author": "Lucas Everett",
  "homepage_url": "https://toolbardial.com"
}
